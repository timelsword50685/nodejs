{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/html/(.*)",
        "dest": "/html/$1"
      },
      {
        "src": "/pictures/(.*)",
        "dest": "/pictures/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
  }